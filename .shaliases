
alias venv="source ~/.venvs/venv/bin/activate$VENV_SUFFIX"
alias ytdl='yt-dlp --extractor-args "youtube:player_client=all"'
alias ag="ag --nogroup"
alias s="vi ~/.local/bin/\$(stest -lx ~/.local/bin | fzf --tac -e)"
alias ffmpeg="ffmpeg -hide_banner"
alias ffprobe="ffprobe -hide_banner"
alias fd="fd --hidden"
alias ls="ls --color=auto"
alias dns="dig +short"
alias fm=clifm
alias tb="nc termbin.com 9999"
alias neovide="neovide --nofork"
alias msetup="meson setup builddir"
alias mcomp="meson compile -C builddir"
alias mtest="meson test -C builddir --verbose"
alias mdev="meson devenv -C build"
alias eix="COLORSCHEME1=4 eix"
alias httpserve='python -m http.server -d'
alias qs="qsearch -HvR"
alias qi="qwhich -pvI"
[ -x /usr/bin/gel ] && alias gel='/usr/bin/gel'
